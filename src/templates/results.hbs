{{#if tableData.length}}
{{!-- {{log tableData}} --}}
<div class="results">
  <table>
    <tbody>
      <tr> 
        <th><img src="/public/images/sort-up-and-down-arrows-couple.svg" alt="sort icon" id='sort-brand'>Brand Name</th>
        <th>Asset Name</th>
        <th>Asset Type</th>
        <th>File Type</th>
        <th>Specs</th>
        <th>Last Updated</th>
        <th>Actions</th>
      </tr>
      {{#each tableData}}
        <tr>
          <td class="result__brand-name">{{brandId}}</td>
          <td class="result__asset-name">{{name}}</td>
          <td class="result__asset-type">{{asset_type}}</td>
          <td class="result-file-type">
            {{selectDropdownFileType (assets, fileType)}}
            {{!-- <select>
              {{#each assets}}
                <option>{{fileType}}</option>
              {{/each}}
            </select> --}}
          </td>
          <td class="result-image__resolution">
            {{selectDropdownResolution assets}}
            {{!-- <select>
              {{#each assets}}
                <option>{{spec.width}} X {{spec.height}} at {{spec.resolution}}</option>
              {{/each}}
            </select> --}}
          </td>
          <td class="result-last__update">{{lastUpdated}}</td>
          <td class="result__actions">
            <div id="copy-link" class="copy-link">Copy Link</div>
            <div id="download" class="download">Download</div>
            <div id="preview" class="preview">Preview</div>
          </td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
{{/if}}
